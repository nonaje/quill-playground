{
  "name": "nonaje/quill-playground",
  "description": "The playground for Quill Framework",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Agustin Ferrari"
    }
  ],
  "require": {
    "php": "^8.4",
    "nonaje/quill-framework": "*@dev",
    "symfony/var-dumper": "^7.0.0"
  },
  "config": {
    "platform": {
      "php": "8.4"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../quill-framework",
      "options": { "symlink": true }
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
    "scripts": {
    "post-install-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "tinker": "php bin/tinker"
  }
}
